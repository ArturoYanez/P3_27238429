	<style type="text/css">
		*{
			margin:0;
			padding:0;
			box-sizing:border-box;
		}
		@font-face{
			font-family:'dar';
			src:url('/fuentes/Gamer.ttf');
		}
		body{
			background-image:url('https://images.unsplash.com/photo-1563248870-e45d2e6df7ee?auto=format&fit=crop&q=80&w=1275&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');
			background-size:cover;
			background-attachment: fixed;
		}
		h1{
			display:flex;
			justify-content: center;
			align-items: center;
			margin:50px auto;
			width:13%;
			font:4rem 'dar';
			color:white;
			text-shadow:15px 15px 5px black;
		}
		form{
			display:flex;
			flex-direction: column;
			justify-content: center;
			align-items: center;
			border:1px solid black;
			margin:50px auto;
			width: 40%;
			padding: 20px;
			height:auto;
			background:rgba(0,0, 0,.7);
			border:1px solid white;
		}
		input{
			padding:6px;
			border-radius:10px ;
			text-align: center;
		}
		label{
			font:1.6rem 'dar';
			margin: 5px;
			color:white;
		}
		#registro{
			cursor: pointer;
			margin-top:20px;
		}
		.g-recaptcha{
			margin-top: 1rem;
		}
		a{
			color:rgb(116, 188, 243);
			text-decoration: none;
		}
		p{
			color:white;
			font-size: 1.2rem;
		}
		@media screen and (max-width:720px){
			form{

				display:flex;
				flex-direction: column;
				justify-content: center;
				align-items: center;
				border:1px solid black;
				margin:50px auto;
				width: 80%;
				padding: 20px;
				height:auto;
				background:rgba(0,0, 0,.7);
				border:1px solid white;
				
			}
		}
	</style>
	<script src="https://www.google.com/recaptcha/api.js" async defer></script>



</head>
<body>
	<h1>Registro</h1>

	<form action="/registroUsuariosPost" method="POST">

		<label>Nombre</label>
		<input type="text" name="nombre" required>

		<label>Apellido</label>
		<input type="text" name="apellido" required>

		<label>Cedula</label>
		<input type="number" name="cedula" required>

		<label>Correo</label>
		<input type="email" name="email" required>

		<label>Contraseña</label>
		<input type="password" name="contrasena" required>
		<div class="g-recaptcha" data-sitekey="6LcoNy0pAAAAANL1RlKBWgjSPc2mn7qI5Gf59Xo3
		"></div>
		<input type="submit" value='Registrar' id="registro">
		<p style='margin-left:70%;margin-bottom:10px;'>Inicia sesion <a href="/loginUsers">Login</a></p>
	</form>
</body>
</html>